{
  "name": "isphording-inneneinrichtung",
  "version": "1.0.0",
  "description": "Online Presence of German Interior Design Company Isphording Inneneinrichtung GmbH",
  "main": "index.php",
  "dependencies": {
    "gsap": "^3.2.6",
    "imagesloaded": "^4.1.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run build:parcel && npm run build:sass && npm run webp",
    "build:sass": "sass assets/css:dist",
    "build:parcel": "parcel build assets/js/script.js -d dist --no-minify --no-source-maps --public-url /assets",
    "watch": "concurrently --kill-others \"npm run watch:sass\" \"npm run watch:parcel\"",
    "watch:sass": "sass --watch assets/css:dist",
    "watch:parcel": "parcel watch assets/js/script.js -d dist /assets",
    "webp": "imagemin --plugin.webp.quality=85 --plugin.webp.preset=photo --plugin.webp.method=6 --plugin.webp.metadata=icc assets/img/* --out-dir=dist/img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jisphording/isphording-inneneinrichtung.git"
  },
  "keywords": [
    "interior",
    "design",
    "architecture",
    "fabrics",
    "furniture"
  ],
  "author": "Johannes Isphording",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jisphording/isphording-inneneinrichtung/issues"
  },
  "homepage": "https://github.com/jisphording/isphording-inneneinrichtung#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "imagemin-webp": "^5.1.0",
    "parcel": "^1.12.4",
    "sass": "^1.26.3"
  }
}
